  "scripts": {
    "test": "mocha --watch test/**.js",
    "build-css": "node-sass assets/stylesheets/styles.scss -o public/stylesheets",
    "build-frontend": "webpack",
    "build-css-dev": "node-sass -w assets/stylesheets/styles.scss -o public/stylesheets",
    "build-frontend-dev": "webpack -w",
    "build-dev": "concurrently --kill-others \"npm run build-css-dev\" \"npm run build-frontend-dev\"",
    "start-dev": "concurrently --kill-others \"npm run build-dev\" \"nodemon -e js ./server/index.js\"",
    "build": "concurrently --kill-others \"npm run build-css\" \"npm run build-frontend\"",
    "start": "node ./server/index.js"
  },
